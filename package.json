{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Node.js boilerplate with Express, TypeScript, and other tools",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "npm run lint:fix",
    "prepush": "npm run test"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.14.2",
    "applicationinsights": "^3.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "pg": "^8.12.0",
    "sequelize": "^6.23.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "sinon": "^15.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.4"
  }
}

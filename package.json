{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Node.js boilerplate with Express, TypeScript, and other tools",
  "main": "dist/index.js",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/migration/InitialMigration -d src/ormconfig.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/ormconfig.ts",
    "start": "nyc ts-node -r tsconfig-paths/register src/index.ts",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "babel src --out-dir dist --extensions \".ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testRegex=unit",
    "test:integration": "jest --testRegex=integration --config jest.integration.config.ts",
    "test:integration:docker": "docker-compose -f docker-compose.it.test.yml up --abort-on-container-exit --exit-code-from app",
    "prepare": "husky && husky install",
    "precommit": "npm run lint:fix",
    "prepush": "npm run test:coverage",
    "coverage": "npx nyc report --reporter=html",
    "report": "xdg-open coverage/index.html"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/k6": "^0.51.0",
    "@types/node": "^18.14.2",
    "applicationinsights": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.20",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-plugin-istanbul": "^6.1.1",
    "c8": "^10.1.1",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^7.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "mocha": "^10.0.0",
    "nyc": "^17.0.0",
    "prettier": "^2.6.2",
    "sinon": "^15.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^4.9.5"
  }
}
